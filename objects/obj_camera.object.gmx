//Blur
band = (blur_spd / (delta_time / 1000000.0));
if (band &gt; 1) {
  if (blur_active) {
    blur_radius += (blur_radius_band - blur_radius) / band;
  } else {
    blur_radius += (0.0 - blur_radius) / band;
  }
} else {
  if (blur_active) {
    blur_radius = blur_radius_band;
  } else {
